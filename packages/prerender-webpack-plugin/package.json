  },
  "devDependencies": {
    "@types/node": "10.14.12",
    "@types/webpack": "4.4.35",
    "eslint": "6.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.18.2",
    "ts-node-dev": "1.0.0-pre.40",
    "typescript": "3.5.3"
  }
}
