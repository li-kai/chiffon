{
  "name": "@chiffon/prerender-webpack-plugin",
  "version": "0.0.2",
  "description": "Prerender for web applications built with Preact",
  "source": "src/index.ts",
  "ts:main": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "{src,dist}/{,!(__*)}/!(*.test)*.{js,ts}"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "fmt": "prettier \"*.{js,ts,json,md}\" \"src/**/*.{js,ts,json,md}\" --write",
    "prepublishOnly": "rm -rf dist && yarn build"
  },
  "devDependencies": {
    "@types/html-minifier": "3.5.3",
    "@types/node": "10.14.13",
    "@types/webpack": "4.32.1",
    "@types/webpack-sources": "0.1.5",
    "eslint": "6.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.18.2",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "html-minifier": "4.0.0",
    "webpack-sources": "1.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
