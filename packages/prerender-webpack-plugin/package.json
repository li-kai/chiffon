{
  "name": "@chiffon/prerender-webpack-plugin",
  "version": "0.0.1",
  "description": "Prerender for web applications built with Preact",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
